<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--TargetType="{x:Type Button}只有按钮才能使用该资源-->
    <ControlTemplate x:Key="MetroTileButtonTemplate" TargetType="{x:Type Button}">
        <Grid x:Name="controlLayout">
            <Rectangle x:Name="buttonSurface" RadiusX="8" RadiusY="8" Fill="{TemplateBinding Background}"/>
            <Label x:Name="buttonCaption" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Foreground="White" Content="{TemplateBinding Content}"/>
        </Grid>

        <!--向模板添加触发器-->
        <ControlTemplate.Triggers>
            <!--Property="IsMouseOver" Value="True" 鼠标经过触发器为True-->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="buttonSurface" Property="Fill" Value="#0089e1"/>
                <Setter TargetName="buttonCaption" Property="Foreground" Value="#ffffff"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    
    <!--TargetType="{x:Type Button}只有按钮才能使用该资源-->
    <ControlTemplate x:Key="RoundButtonTemplate" TargetType="{x:Type Button}">
        <Grid x:Name="controlLayout">
            <Rectangle x:Name="buttonSurface" RadiusX="8" RadiusY="8" Fill="{TemplateBinding Background}"/>
            <Label x:Name="buttonCaption" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Foreground="White" Content="{TemplateBinding Content}"/>
        </Grid>

        <!--向模板添加触发器-->
        <ControlTemplate.Triggers>
            <!--Property="IsMouseOver" Value="True" 鼠标经过触发器为True-->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="buttonSurface" Property="Fill" Value="#0089e1"/>
                <Setter TargetName="buttonCaption" Property="Foreground" Value="#ffffff"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--菜单按钮控件-->
    <ControlTemplate x:Key="MenuButtonTemplate" TargetType="{x:Type Button}">
        <Grid x:Name="controlLayout" Margin="0,5,0,0">
            <Rectangle x:Name="buttonSurface" RadiusX="3" RadiusY="3" Fill="{TemplateBinding Background}"/>
            <Label x:Name="buttonCaption"  HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Foreground="White" Content="{TemplateBinding Content}"/>
        </Grid>

        <!--向模板添加触发器-->
        <ControlTemplate.Triggers>
            <!--Property="IsMouseOver" Value="True" 鼠标经过触发器为True-->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="buttonSurface" Property="Fill" Value="#cccccc"/>
                <Setter TargetName="buttonCaption" Property="Foreground" Value="#ffffff"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--按钮控件-->
    <ControlTemplate x:Key="ButtonTemplate" TargetType="{x:Type Button}">
        <Grid x:Name="controlLayout" Margin="0,5,0,0">
            <Rectangle x:Name="buttonSurface" RadiusX="3" RadiusY="3" Fill="{TemplateBinding Background}"/>
            <Label x:Name="buttonCaption"  HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Foreground="White" Content="{TemplateBinding Content}"/>
        </Grid>

        <!--向模板添加触发器-->
        <ControlTemplate.Triggers>
            <!--Property="IsMouseOver" Value="True" 鼠标经过触发器为True-->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="buttonSurface" Property="Fill" Value="#cccccc"/>
                <Setter TargetName="buttonCaption" Property="Foreground" Value="#ffffff"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--下拉菜单样式-->
    <Style TargetType="ComboBoxItem" x:Key="ComboBoxItemStyle">
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="RenderOptions.ClearTypeHint" Value="Enabled" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Height" Value="28" />
    </Style>
    
